{
  "name": "semantic-ui-ember",
  "version": "3.0.5",
  "description": "The Official Semantic UI Addon for Ember applications",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "",
  "title": "Semantic UI Ember",
  "homepage": "http://www.semantic-ui.com",
  "contributors": [
    {
      "name": "Arnav Gupta",
      "email": "dev@championswimmer.in"
    },
    {
      "name": "Nathan Palmer",
      "email": "nathan@nathanpalmer.com"
    },
    {
      "name": "Aaron Hansen",
      "email": "aaronbhansen@gmail.com"
    }
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/lssjbrolli/Semantic-UI-Ember.git"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js:fix": "eslint . --fix",
    "start": "ember server",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "gh-pages": "ember build && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages -f"
  },
  "dependencies": {
    "@ember/jquery": "^2.0.0",
    "@ember/render-modifiers": "^1.0.2",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "semantic-ui-css": "^2.4.1",
    "walk-sync": "^3.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.8",
    "@embroider/test-setup": "^0.42.2",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-stew": "^3.0.0",
    "ember-auto-import": "^2.0.2",
    "ember-cli": "~3.27.0",
    "ember-cli-clipboard": "^0.15.0",
    "ember-cli-code-coverage": "^1.0.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-code-snippet": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.27.5",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.5.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-qunit": "^6.1.1",
    "highlightjs": "^9.16.2",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "qunit": "^2.16.0",
    "qunit-dom": "^1.6.0",
    "tape": "^5.2.2"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://Semantic-Org.github.io/Semantic-UI-Ember"
  }
}
